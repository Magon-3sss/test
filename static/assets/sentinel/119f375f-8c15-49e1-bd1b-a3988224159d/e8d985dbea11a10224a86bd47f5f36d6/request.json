        "evalscript": "\n           //VERSION=3\nfunction setup() {\n    return {\n        input: [\"B08\", \"B04\", \"B11\", \"dataMask\"],\n        output: { bands: 4 }\n    };\n}\n\nfunction evaluatePixel(sample) {\n    // Calculate NDVI and NDMI indices\n    let ndvi = index(sample.B08, sample.B04);\n    let ndmi = index(sample.B08, sample.B11);\n    \n    // Debug conditions: Apply different colors for each condition\n    if (ndvi < 0.2 && ndmi > 0.4) {\n        // Red for detected anomaly (low NDVI and high NDMI)\n        return [1, 0, 0, sample.dataMask];\n    } else if (ndvi >= 0.2 && ndvi <= 0.6 && ndmi <= 0.4) {\n        // Yellow for moderate NDVI (example for different ranges)\n        return [1, 1, 0, sample.dataMask];\n    } else if (ndvi > 0.6) {\n        // Blue for high NDVI (example to indicate healthy vegetation)\n        return [0, 0, 1, sample.dataMask];\n    } else {\n        // Green for non-anomalous areas\n        return [0, 1, 0, sample.dataMask];\n    }\n}\n\n        ",
